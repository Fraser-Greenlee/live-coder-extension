{
    "name": "live-coder",
    "displayName": "Live Coder",
    "description": "See how your code executes as you write it.",
    "version": "1.0.2",
    "publisher": "Fraser",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Debuggers",
        "Linters",
        "Other"
    ],
    "activationEvents": [
        "onCommand:liveValues.start",
        "onWebviewPanel:liveValues"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "liveValues.start",
                "title": "Open Live Coder",
                "category": "Live Coder"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.34"
    },
    "dependencies": {
        "@types/request-promise-native": "^1.0.16",
        "applicationinsights": "^1.4.0",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.7",
        "vscode-extension-telemetry": "^0.1.2"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/Fraser-Greenlee/live-coder-vscode-extension.git"
    },
    "icon": "logo.png"
}
